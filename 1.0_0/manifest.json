{
   "background": {
      "page": "background.html"
   },
   "action": {
      "default_icon": "pics/workspaces32.png",
      "default_popup": "popup.html",
      "default_title": "Chrome Workspaces"
   },
   "description": "Easy to use session manager. Name a window and now you can open the last set of tabs that you had open in that session.",
   "icons": {
      "128": "pics/workspaces128.png",
      "48": "pics/workspaces48.png"
   },
   "manifest_version": 3,
   "name": "Chrome Workspaces",
   "options_page": "options.html",
   "permissions": [ "bookmarks", "storage", "tabs", "unlimitedStorage", "ftp://*/*", "http://*/*", "https://*/*" ],
   "version": "1.0.0"
}
